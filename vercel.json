{
  "version": 2,
  "builds": [
    {
      "src": "api/app.js", 
      "use": "@vercel/node"
    },
    {
      "src": "front-end/**/*", 
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/app.js" 
    },
    {
      "src": "/(.*)", 
      "dest": "/front-end/$1" 
    }
  ]
}
