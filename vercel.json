{
  "version": 2,
  "builds": [
    {
      "src": "back-end/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "front-end/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "back-end/app.js"
    },
    {
      "src": "/routes/(.*)",
      "dest": "back-end/routes/$1"
    },
    {
      "src": "/(.*)",
      "dest": "front-end/$1"
    }
  ]
}

